page {
  /* background-color: #F8F8F8; */
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}

.page-section-spacing {
  box-sizing: border-box;
  padding: 0 20rpx;
}

.swiper-item {
  display: block;
  width: 100%;
  height: 300rpx;
  border-radius: 10rpx;
}

.swiper-item image {
  width: 100%;
  height: 100%;
}

.category-box {
  background-color: #fff;
  display: flex;
  flex-wrap: wrap;
}

.category-list {
  width: 150rpx;
  margin-top: 20rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}

.category-column {
  width: 100%;
  margin-top: 20rpx;
  overflow: hidden;
}

.category-imgbox {
  width: 80rpx;
  height: 80rpx;
}

.category-title {
  font-size: 24rpx;
  text-align: center;
  margin-top: 10rpx;
}

.coupon-box{
  padding: 20rpx;
  background-color: #f2f2f2;
}

.coupon-wrapper{
  margin: 0 auto;
  width: 100%;
  display: flex;
  background: linear-gradient(-90deg,rgba(250,173,82,1),rgba(254,50,103,1));
}

.coupon-content{
  position: relative;
  flex: 1;
  padding: 20rpx 30rpx;
  text-align: center;
}

.coupon-content::before, .coupon-content::after{
  content: '';
  position: absolute;
  width: 32rpx;
  height: 16rpx;
  background: #f2f2f2;
  left: -16rpx;
  z-index: 1;
}

.coupon-tip{
  position: relative;
  padding: 50rpx 30rpx;
  flex: 0 0 200rpx;
  text-align: center;
}

.coupon-tip::before, .coupon-tip::after{
  content: '';
  position: absolute;
  width: 32rpx;
  height: 16rpx;
  background: #f2f2f2;
  left: -16rpx;
  z-index: 1;
}

.split-line{
  position: relative;
  flex: 0 0 0;
  margin: 0 10rpx 0 6rpx;
  border-left: 4rpx dashed #f2f2f2;
}

.split-line::before, .split-line::after{
  content: '';
  position: absolute;
  width: 32rpx;
  height: 16rpx;
  background: #f2f2f2;
  left: -18rpx;
  z-index: 1;
}

.coupon-content::before, .coupon-tip::before, .split-line::before{
  border-radius: 0 0 16rpx 16rpx;
  top: 0;
}

.coupon-content::after, .coupon-tip::after, .split-line::after{
  border-radius: 16rpx 16rpx 0 0;
  bottom: 0;
}

.coupon-money{
  font-size: 80rpx;
  color: #f2f2f2;
}

.coupon-money text{
  font-size: 50rpx;
  padding-right: 20rpx;
}

.description-title{
  color: #f2f2f2;
  font-size: 30rpx;
}

.description-title{
  color: #f2f2f2;
  font-size: 30rpx;
}

.coupon-conditions{
  color: #f2f2f2;
  font-size: 30rpx;
  padding: 15rpx;
}

.coupon-useNow{
  width: 110rpx;
  color: rgba(254, 50, 103, 1);
  font-size: 30rpx;
  border-radius: 50rpx;
  border-style: none;
}

.coupon-description{
  padding-top: 10rpx;
}

.coupon-description .left-line{
  width: 64rpx;
  height: 1rpx;
  background: #fff;
  margin-left: 60rpx;
  margin-bottom: -40rpx;
}

.coupon-description .right-line{
  width: 64rpx;
  height: 1rpx;
  background: #fff;
  margin-left: 260rpx;
  margin-bottom: -15rpx;
}
