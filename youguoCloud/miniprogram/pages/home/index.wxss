.img_wrap {
  height: 82rpx;
  margin-top: 10rpx;
}

.img_wrap image {
  width: 100%;
  height: 100%;
}

.goods-container {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: content-box;
  padding: 24rpx;
  background-color: #F2f2f2;
}

.goods-box {
  width: 339rpx;
  height: 510rpx;
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 24rpx;
  border-radius: 5px;
}

.goods-box .img-box {
  width: 339rpx;
  height: 339rpx;
  overflow: hidden;
}

.goods-box .img-box image {
  width: 339rpx;
  height: 339rpx;
}

.goods-box .goods-title {
  width: 280rpx;
  text-overflow: ellipsis;
  white-space: normal;
  overflow: hidden;
  font-size: 24rpx;
  padding: 24rpx 0 0rpx 0;
  color: #000;
  margin-left: 24rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.goods-box .goods-price {
  width: 280rpx;
  overflow: hidden;
  font-size: 24rpx;
  padding: 24rpx 0;
  color: #e64340;
  margin-left: 24rpx;
}

.goods-box .goods-img {
  width: 150rpx;
  height: 57rpx;
  margin-right: 10rpx;
  margin-top: 10rpx;
}

.goods-box .goods-img image {
  width: 100%;
  height: 100%;
}

.cutdown-for-tab {
  background-color: #F2f2f2;
  display: flex;
  margin-bottom: -20rpx;
  padding: 0 10rpx;
}

.left-text {
  flex: 2;
  text-align: left;
  font-size: 28rpx;
  color: #000;
}

.right-txt {
  flex: 1;
  text-align: right;
  font-size: 28rpx;
  color: #000;
}

.countdown-item {
  background-color: #e93b3d;
  color: #fff;
  padding: 0 5rpx;
  border-radius: 3rpx;
}